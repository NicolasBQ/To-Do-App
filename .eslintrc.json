{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-standard",
        "prettier"
      ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "never"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
